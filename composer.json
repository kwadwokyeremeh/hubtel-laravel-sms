{
    "name": "kwadwokyeremeh/hubtel-laravel-sms",
    "description": "Hubtel SMS Channel for Laravel ^11|^122",
    "homepage": "https://github.com/norris1z/hubtel-laravel-sms-channel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "sms",
        "hubtel"
    ],
    "authors": [
        {
            "name": "Kwadwo Kyeremeh",
            "email": "kyerematics@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8|^7.9",
        "illuminate/notifications": "^11|^12",
        "illuminate/support": "^11|^12"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Hubtel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Hubtel\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Hubtel\\HubtelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
