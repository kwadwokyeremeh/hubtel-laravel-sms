{
    "name": "kwadwokyeremeh/hubtel-laravel-sms",
    "description": "Hubtel SMS Channel for Laravel ^11",
    "homepage": "https://github.com/norris1z/hubtel-laravel-sms-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kwadwo Kyeremeh",
            "email": "kyerematics@gmail.com",
            "homepage": "https://norrisoduro.carrd.co",
            "role": "Explicit coder at Volatile"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/notifications": "^11.*",
        "illuminate/support": "^11.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Hubtel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Hubtel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Hubtel\\HubtelServiceProvider"
            ]
        }
    }
}
